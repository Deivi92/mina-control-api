    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "@mui/x-data-grid": "^8.12.1",
    "@tanstack/react-query": "^5.85.5",
    "axios": "^1.11.0",
    "formik": "^2.4.6",
    "jwt-decode": "^4.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.63.0",
    "react-router-dom": "^7.8.2",
    "yup": "^1.7.0",
